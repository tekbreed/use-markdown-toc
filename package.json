    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitest/coverage-v8": "^1.3.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
