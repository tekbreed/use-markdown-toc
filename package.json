  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christophersesugh/use-markdown-toc.git"
  },
  "homepage": "https://github.com/christophersesugh/use-markdown-toc#readme",
  "bugs": {
    "url": "https://github.com/christophersesugh/use-markdown-toc/issues"
